{


	"name": "Joxi Viewer","version": "1.0","manifest_version": 2,

    "permissions": [
        "storage",
        "tabs",
        "<all_urls>"
    ],

	"content_scripts": [{
		"all_frames": true,
		"matches": ["http://*/*","https://*/*", "file:///*/*"],
		"js": ["jquery-1.8.2.min.js", "newjscript.user.js"]
	}]


}





